@model IEnumerable<proyectoFinal.Models.consulta>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            <label>Fecha de consulta</label>
        </th>
        <th>
            <label>Diagnostico </label>
        </th>
        <th>
            <label>Observaciones </label>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cita.fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.diagnostico)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.observaciones)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.codconsulta }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.codconsulta }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.codconsulta })
        </td>
    </tr>
}

</table>
